        <<dataclass>>
        + url: str
        + whatcms_link: str
        + blog_cms: List[str]
        + ecommerce_cms: List[str]
        + programming_language: List[str]
        + database: List[str]
        + cdn: List[str]
        + web_server: List[str]
        + landing_page_builder_cms: List[str]
        + operating_system: List[str]
        + web_framework: List[str]
