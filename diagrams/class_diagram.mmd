    class ConfigFile {
        <<artifact>>
        + api_key : str
        + input_file : str
        + output_file : str
    }

    class WhatCMSClient {
        - BASE_URL: str
        - api_key: str
        - rate_limit_delay: float
        - session: Session
        + __init__(api_key: str, rate_limit_delay: float)
        + fetch_cms_data(url: str): WhatCMSResponse
        - _parse_response(url: str, data: Dict): WhatCMSResponse
        - _extract_category_data(response_obj: WhatCMSResponse, category: Dict)
        - _clean_tech_category(technologies: List[str]): str
        + close()
    }

    class DataEnricher {
        - client: WhatCMSClient
        + __init__(api_key: str)
        + load_input_data(file_path: str, sheet_name: str): DataFrame
        + enrich_urls(urls: List[str]): List~WhatCMSResponse~
        + enrich_dataframe(df: DataFrame, url_column: str): DataFrame
        - _responses_to_dataframe(responses: List~WhatCMSResponse~): DataFrame
        + save_output(df: DataFrame, output_path: str)
        + run_enrichment_workflow(input_file: str, output_file: str, sheet_name: str, url_column: str)
    }

    class main {
        <<function>>
        + main()
    }

    WhatCMSClient --> WhatCMSResponse : creates/returns
    DataEnricher *-- WhatCMSClient : composition
    DataEnricher --> WhatCMSResponse : uses
    main --> DataEnricher : creates/uses
    ConfigFile --> main : provides parameters

